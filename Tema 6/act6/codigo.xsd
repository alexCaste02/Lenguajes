<?xml version="1.0" encoding="UTF-8"?>
<xsi:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema">
  <xsi:element name="codigo">
    <xsi:simpleType>
      <xsi:restriction base="xsi:string">
        <xsi:pattern value="[A-Z]\d{3}[A-Z]{2}"/>
      </xsi:restriction>
    </xsi:simpleType>
  </xsi:element>
</xsi:schema>

